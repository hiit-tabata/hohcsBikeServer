{
  "name": "dataSample",
  "base": "Model",
  "idInjection": true,
  "properties": {
    "dateTime": {
      "type": "date",
      "required": true
    },
    "sensorType": {
      "type": "string",
      "required": true
    },
    "name": {
      "type": "string"
    },
    "value": {
      "type": [
        "number"
      ],
      "required": true
    },
    "sensorId": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "linked": {
      "type": "belongsTo",
      "polymorphic": {
        "idType": "number"
      },
      "properties": {
        "name": "name"
      },
      "options": {
        "invertProperties": true
      }
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
